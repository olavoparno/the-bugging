{
  "name": "the-bugging",
  "version": "1.0.16",
  "description": "TheBugging is a simple and lightweight library for logging and debugging your code.",
  "keywords": [],
  "author": "Olavo Parno",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/olavoparno/the-bugging.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "lint-staged": {
    "*.ts": [
      "eslint --fix src/**/*.ts"
    ]
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "yarn prebuild && rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "lint": "eslint src --fix --cache",
    "prepublish": "yarn build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/runtime": "^7.17.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-terser": "^0.2.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "all-contributors-cli": "^6.24.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.8.1",
    "rollup": "^2.67.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "standard-version": "^9.5.0",
    "tslib": "^2.3.1",
    "typescript": "*"
  }
}
